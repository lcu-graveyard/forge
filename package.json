{
	"name": "@napkin-ide/forge",
	"version": "0.0.5",
	"scripts": {
		"build": "npm run build:all",
		"build:all": "npm run build:common && concurrently \"npm:build:public\" \"npm:build:ide\"",
		"build:common": "ng build common",
		"build:ide": "ng build ide --prod",
		"build:public": "ng build public --prod",
		"deploy-man": "npm run deploy && npm run git:clean",
		"deploy": "npm run switch && npm run versions && npm run build && npm run deploy:all",
		"deploy:all": "concurrently \"npm:deploy:common\" \"npm:deploy:public\" \"npm:deploy:ide\" \"npm:deploy:org-reg\"",
		"deploy:common": "npm publish ./dist/common --access public",
		"deploy:ide": "npm publish ./dist/ide --access public",
		"deploy:org-reg": "ng build org-reg --prod && npm publish ./dist/org-reg --access public",
		"deploy:public": "npm publish ./dist/public --access public",
		"git:clean": "git add . && git commit -a -m \"Changes during manual deploy\"",
		"switch": "npm run switch:all",
		"switch:all": "concurrently \"npm:switch:public\" \"npm:switch:ide\"",
		"switch:common": "lcu daf Switch --project=common --env=prod",
		"switch:ide": "lcu daf Switch --project=ide --env=prod",
		"switch:public": "lcu daf Switch --project=public --env=prod",
		"versions": "npm version patch && npm run version:all",
		"version:all": "concurrently \"npm:version:common\" \"npm:version:public\" \"npm:version:ide\"",
		"version:common": "npm version patch --prefix projects/common",
		"version:ide": "npm version patch --prefix projects/ide",
		"version:public": "npm version patch --prefix projects/public",
		"ng": "ng",
		"start": "ng serve",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"module": "ng g module ___ --project=___",
		"comp": "ng g component ___ --project=___ -m ___"
	},
	"private": false,
	"dependencies": {
		"@angular/animations": "~7.2.0",
		"@angular/cdk": "~7.2.1",
		"@angular/common": "~7.2.0",
		"@angular/compiler": "~7.2.0",
		"@angular/core": "~7.2.0",
		"@angular/flex-layout": "^7.0.0-beta.23",
		"@angular/forms": "~7.2.0",
		"@angular/http": "^7.2.1",
		"@angular/material": "^7.2.1",
		"@angular/platform-browser": "~7.2.0",
		"@angular/platform-browser-dynamic": "~7.2.0",
		"@angular/router": "~7.2.0",
		"@aspnet/signalr": "^1.1.2",
		"@lcu-ide/common": "0.0.7",
		"@lcu/api": "^0.1.18-integration",
		"@lcu/apps": "^0.1.18-integration",
		"@lcu/common": "^0.1.18-integration",
		"@lcu/core": "^0.1.18-integration",
		"@lcu/daf-common": "^0.1.65-integration",
		"@lcu/elements": "^0.1.18-integration",
		"@lcu/enterprises": "^0.1.5-integration",
		"@lcu/hosting": "^0.1.18-integration",
		"@lcu/identity": "^0.1.44-integration",
		"@lcu/routing": "^0.1.18-integration",
		"@lowcodeunit-devkit/ng-lcu": "0.0.167",
		"core-js": "^2.5.4",
		"dot-object": "^1.7.1",
		"hammerjs": "^2.0.8",
		"rxjs": "~6.3.3",
		"tslib": "^1.9.0",
		"zone.js": "~0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.12.0",
		"@angular-devkit/build-ng-packagr": "~0.12.0",
		"@angular/cli": "~7.2.1",
		"@angular/compiler-cli": "~7.2.0",
		"@angular/language-service": "~7.2.0",
		"@lcu/cli": "0.0.86",
		"@types/jasmine": "~2.8.8",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "~8.9.4",
		"codelyzer": "~4.5.0",
		"concurrently": "^4.1.0",
		"jasmine-core": "~2.99.1",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~3.1.1",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.1",
		"karma-jasmine": "~1.1.2",
		"karma-jasmine-html-reporter": "^0.2.2",
		"ng-packagr": "^4.2.0",
		"protractor": "~5.4.0",
		"ts-node": "~7.0.0",
		"tsickle": ">=0.34.0",
		"tslib": "^1.9.0",
		"tslint": "~5.11.0",
		"typescript": "~3.2.2"
	}
}
